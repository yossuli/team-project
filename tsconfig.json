{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "types": ["@cloudflare/workers-types", "vite/client", "vitest/importMeta"],
    "jsx": "react-jsx",
    "baseUrl": ".",
    "jsxImportSource": "react",
    "paths": {
      "~/*": ["./client/*"],
      "@ss/*": ["./styled-system/*"]
    }
  },
  "include": ["styled-system", "app", "client"]
}
